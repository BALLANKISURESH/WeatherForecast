<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>Log</servlet-name>
  	<display-name>Log</display-name>
  	<description></description>
  	<servlet-class>com.virtusa.verify.Log</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>Veripass</servlet-name>
  	<display-name>Veripass</display-name>
  	<description></description>
  	<servlet-class>com.virtusa.verify.Veripass</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>WeatherServices</servlet-name>
  	<display-name>WeatherServices</display-name>
  	<description></description>
  	<servlet-class>com.virtusa.weather.WeatherServices</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>HistoricalDataBetween</servlet-name>
  	<display-name>HistoricalDataBetween</display-name>
  	<description></description>
  	<servlet-class>com.virtusa.weather.HistoricalDataBetween</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>Logout</servlet-name>
  	<display-name>Logout</display-name>
  	<description></description>
  	<servlet-class>com.virtusa.verify.Logout</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>Log</servlet-name>
  	<url-pattern>/log</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>Veripass</servlet-name>
  	<url-pattern>/Verifypwd</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>WeatherServices</servlet-name>
  	<url-pattern>/WeatherServices</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>HistoricalDataBetween</servlet-name>
  	<url-pattern>/perioddata</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>Logout</servlet-name>
  	<url-pattern>/logout</url-pattern>
  </servlet-mapping>
</web-app>
